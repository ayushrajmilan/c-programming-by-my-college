// 104. print fibbonacci series in recursion

#include <stdio.h>

// Recursive function to  print fibbonacci series
int fac(int n);
int fac(int n) {
    if (n == 0)   return 0;        
    if (n == 1) return 1;       
    return fac(n-1)+fac(n-2);
    }

int main() {
    int n;
    printf("Enter a number: ");
    scanf("%d", &n);
    
    for (int i=0;i<n;i++){
    printf(" %d ", fac(i));}

    return 0;
}
