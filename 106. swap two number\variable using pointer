
// 106. swap two number\variable using pointer 
#include <stdio.h>

// int swap(int,int);
int swap(int *a,int *b){
    int temp=*a;
    *a=*b;
    *b=temp;
}

int main() {
    int x,y;
    printf("enter the value of x and y :\n");
    scanf("%d%d",&x,&y);
    swap(&x,&y);
    printf("after swap:x=%d, y=%d\n",x,y);

    return 0;
}
