// 40.Find the greatest of three numbers using Nested if
#include<stdio.h>
int main(){
    int a,b,c,d,e,f=0,g=1;
    printf("enter the first number : second number  :  third number :");
    scanf("%d%d%d",&a,&b,&c);
   if (a>b){
       if(a>c){
           printf("a is the greatest no.%d",a);
       }
       else {
           printf("c is the greatest no %d",c);
       }
   }
   else if(b>c){
       if(b>a){
       printf("b is the greatest no %d",b);}
       else {
       printf("c is the greatest no, %d",c);}
   }
   else {
       printf(" a is the greatest number %d",c);
   }
    return 0;
}
